#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/coffee" ]; then
  "$basedir/coffee"  "$basedir/../haml-coffee/bin/haml-coffee" "$@"
  ret=$?
else 
  coffee  "$basedir/../haml-coffee/bin/haml-coffee" "$@"
  ret=$?
fi
exit $ret
